<% if (user) { %>
	<script>
		<% if (user.background) { %>
			var timestamp = new Date().getTime();
			var pic = "<%=user.background%>".trim().split(";");
			var k = pic.length;
			var tmp;
			var t = Math.floor(timestamp / 100000);
			for (var i = 0; i < 10; i++) {
				t = (t * 1103515245 + 12345);
				t = (Math.floor(t / 65536)) % 32768;
			}
			t = t % k;
			document.body.style.background = "url('" + pic[t] + "') fixed center";
			document.body.style.backgroundSize = "cover";
		<% } %>
	</script>
<% } %>
